<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_marginStart="16dp"
    android:layout_marginLeft="16dp"
    android:layout_marginTop="8dp"
    android:layout_marginEnd="16dp"
    android:layout_marginRight="16dp"
    android:layout_marginBottom="8dp"
    app:layout_constraintBottom_toBottomOf="parent"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toTopOf="parent">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/praters365_timings_box"
        android:layout_width="match_parent"
        android:layout_height="290dp"
        app:layout_constraintTop_toTopOf="parent">

        <LinearLayout
            android:id="@+id/fajrCard"
            android:layout_width="match_parent"
            android:layout_height="55dp"
            android:layout_marginStart="1dp"
            android:layout_marginEnd="1dp"
            app:cardBackgroundColor="?attr/backgroundColor"
            app:layout_constraintTop_toTopOf="parent"
            app:cardCornerRadius="0dp"
            app:strokeWidth="0dp"
            tools:layout_editor_absoluteX="8dp">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <!-- Left: Icon + Prayer Name + Time (Vertical) -->
                <LinearLayout
                    android:id="@+id/fajr_left_container"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="10dp"
                    android:orientation="horizontal"
                    android:gravity="center_vertical"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent">

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:background="@drawable/symbol_fajr" />

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="12dp"
                        android:orientation="vertical"
                        android:gravity="start">

                        <TextView
                            android:id="@+id/fajr_label_text_view"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/FAJR"
                            android:textAppearance="@style/prayer_name"
                            android:textSize="15sp"
                            android:textStyle="bold" />

                        <TextView
                            android:id="@+id/fajr_timing_text_view"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="2dp"
                            android:text="04:54 AM"
                            android:textSize="12sp"
                            android:textAppearance="@style/prayer_name" />
                    </LinearLayout>
                </LinearLayout>

                <!-- Right: Track Button + Reminder Icon -->
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="2dp"
                    android:orientation="horizontal"
                    android:gravity="center"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="parent">

                    <!-- Track Button -->
                    <com.google.android.material.button.MaterialButton
                        android:id="@+id/fajr_track_button"
                        android:layout_width="wrap_content"
                        android:layout_height="32dp"
                        android:layout_marginEnd="8dp"
                        android:text="Track"
                        android:textSize="11sp"
                        android:textColor="@android:color/white"
                        android:paddingStart="12dp"
                        android:paddingEnd="12dp"
                        android:minWidth="0dp"
                        android:insetTop="0dp"
                        android:insetBottom="0dp"
                        android:visibility="visible"
                        app:cornerRadius="16dp"
                        app:backgroundTint="@color/salah_track_button"
                        style="@style/Widget.MaterialComponents.Button" />

                    <!-- Reminder Icon -->
                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/fajr_call_constraint_layout"
                        android:layout_width="38dp"
                        android:layout_height="38dp"
                        android:layout_marginStart="4dp">

                        <ImageView
                            android:id="@+id/fajr_call_image_view"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:contentDescription="@string/title_adhan"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent"
                            android:src="@drawable/symbol_notification" />
                    </androidx.constraintlayout.widget.ConstraintLayout>
                </LinearLayout>

            </androidx.constraintlayout.widget.ConstraintLayout>

        </LinearLayout>


        <LinearLayout
            android:id="@+id/dhuhrCard"
            android:layout_width="match_parent"
            android:layout_height="55dp"
            android:layout_marginStart="1dp"
            android:layout_marginEnd="1dp"
            app:cardBackgroundColor="?attr/backgroundColor"
            app:layout_constraintTop_toBottomOf="@+id/fajrCard"
            app:cardCornerRadius="0dp"
            app:strokeWidth="0dp"
            tools:layout_editor_absoluteX="8dp">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <LinearLayout
                    android:id="@+id/dhuhr_left_container"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="10dp"
                    android:orientation="horizontal"
                    android:gravity="center_vertical"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent">

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:background="@drawable/symbol_dhuhr" />

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="12dp"
                        android:orientation="vertical"
                        android:gravity="start">

                        <TextView
                            android:id="@+id/dohr_label_text_view"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/DHOHR"
                            android:textAppearance="@style/prayer_name"
                            android:textSize="15sp"
                            android:textStyle="bold" />

                        <TextView
                            android:id="@+id/dohr_timing_text_view"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="2dp"
                            android:text="11:57 AM"
                            android:textSize="12sp"
                            android:textAppearance="@style/prayer_name" />
                    </LinearLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="2dp"
                    android:orientation="horizontal"
                    android:gravity="center"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="parent">

                    <com.google.android.material.button.MaterialButton
                        android:id="@+id/dhuhr_track_button"
                        android:layout_width="wrap_content"
                        android:layout_height="32dp"
                        android:layout_marginEnd="8dp"
                        android:text="Track"
                        android:textSize="11sp"
                        android:textColor="@android:color/white"
                        android:paddingStart="12dp"
                        android:paddingEnd="12dp"
                        android:minWidth="0dp"
                        android:insetTop="0dp"
                        android:insetBottom="0dp"
                        android:visibility="visible"
                        app:cornerRadius="16dp"
                        app:backgroundTint="@color/salah_track_button"
                        style="@style/Widget.MaterialComponents.Button" />

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/dohr_call_constraint_layout"
                        android:layout_width="38dp"
                        android:layout_height="38dp"
                        android:layout_marginStart="4dp">

                        <ImageView
                            android:id="@+id/dohr_call_image_view"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@drawable/symbol_notification"
                            android:layout_gravity="center"
                            android:contentDescription="@string/title_adhan"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent"/>
                    </androidx.constraintlayout.widget.ConstraintLayout>
                </LinearLayout>

            </androidx.constraintlayout.widget.ConstraintLayout>

        </LinearLayout>

        <LinearLayout
            android:id="@+id/asrCard"
            android:layout_width="match_parent"
            android:layout_height="55dp"
            android:layout_marginStart="1dp"
            android:layout_marginEnd="1dp"
            app:cardBackgroundColor="?attr/backgroundColor"
            app:layout_constraintTop_toBottomOf="@+id/dhuhrCard"
            app:cardCornerRadius="0dp"
            app:strokeWidth="0dp"
            tools:layout_editor_absoluteX="8dp">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <LinearLayout
                    android:id="@+id/asr_left_container"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="10dp"
                    android:orientation="horizontal"
                    android:gravity="center_vertical"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent">

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:background="@drawable/symbol_asr" />

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="12dp"
                        android:orientation="vertical"
                        android:gravity="start">

                        <TextView
                            android:id="@+id/asr_label_text_view"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/ASR"
                            android:textAppearance="@style/prayer_name"
                            android:textSize="15sp"
                            android:textStyle="bold" />

                        <TextView
                            android:id="@+id/asr_timing_text_view"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="2dp"
                            android:text="3:04 PM"
                            android:textSize="12sp"
                            android:textAppearance="@style/prayer_name" />
                    </LinearLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="2dp"
                    android:orientation="horizontal"
                    android:gravity="center"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="parent">

                    <com.google.android.material.button.MaterialButton
                        android:id="@+id/asr_track_button"
                        android:layout_width="wrap_content"
                        android:layout_height="32dp"
                        android:layout_marginEnd="8dp"
                        android:text="Track"
                        android:textSize="11sp"
                        android:textColor="@android:color/white"
                        android:paddingStart="12dp"
                        android:paddingEnd="12dp"
                        android:minWidth="0dp"
                        android:insetTop="0dp"
                        android:insetBottom="0dp"
                        android:visibility="visible"
                        app:cornerRadius="16dp"
                        app:backgroundTint="@color/salah_track_button"
                        style="@style/Widget.MaterialComponents.Button" />

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/asr_call_constraint_layout"
                        android:layout_width="38dp"
                        android:layout_height="38dp"
                        android:layout_marginStart="4dp">

                        <ImageView
                            android:id="@+id/asr_call_image_view"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@drawable/symbol_notification"
                            android:layout_gravity="center"
                            android:contentDescription="@string/title_adhan"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent"/>
                    </androidx.constraintlayout.widget.ConstraintLayout>
                </LinearLayout>

            </androidx.constraintlayout.widget.ConstraintLayout>

        </LinearLayout>

        <LinearLayout
            android:id="@+id/maghribCard"
            android:layout_width="match_parent"
            android:layout_height="55dp"
            android:layout_marginStart="1dp"
            android:layout_marginEnd="1dp"
            app:cardBackgroundColor="?attr/backgroundColor"
            app:layout_constraintTop_toBottomOf="@+id/asrCard"
            app:cardCornerRadius="0dp"
            app:strokeWidth="0dp"
            tools:layout_editor_absoluteX="8dp">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <LinearLayout
                    android:id="@+id/maghrib_left_container"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="10dp"
                    android:orientation="horizontal"
                    android:gravity="center_vertical"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent">

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:background="@drawable/symbol_maghrib"  />

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="12dp"
                        android:orientation="vertical"
                        android:gravity="start">

                        <TextView
                            android:id="@+id/maghrib_label_text_view"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/MAGHRIB"
                            android:textAppearance="@style/prayer_name"
                            android:textSize="15sp"
                            android:textStyle="bold" />

                        <TextView
                            android:id="@+id/maghreb_timing_text_view"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="2dp"
                            android:text="5:30 PM"
                            android:textSize="12sp"
                            android:textAppearance="@style/prayer_name" />
                    </LinearLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="2dp"
                    android:orientation="horizontal"
                    android:gravity="center"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="parent">

                    <com.google.android.material.button.MaterialButton
                        android:id="@+id/maghrib_track_button"
                        android:layout_width="wrap_content"
                        android:layout_height="32dp"
                        android:layout_marginEnd="8dp"
                        android:text="Track"
                        android:textSize="11sp"
                        android:textColor="@android:color/white"
                        android:paddingStart="12dp"
                        android:paddingEnd="12dp"
                        android:minWidth="0dp"
                        android:insetTop="0dp"
                        android:insetBottom="0dp"
                        android:visibility="visible"
                        app:cornerRadius="16dp"
                        app:backgroundTint="@color/salah_track_button"
                        style="@style/Widget.MaterialComponents.Button" />

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/maghreb_call_constraint_layout"
                        android:layout_width="38dp"
                        android:layout_height="38dp"
                        android:layout_marginStart="4dp">

                        <ImageView
                            android:id="@+id/maghreb_call_image_view"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:contentDescription="@string/title_adhan"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent"
                            android:src="@drawable/symbol_notification" />
                    </androidx.constraintlayout.widget.ConstraintLayout>
                </LinearLayout>

            </androidx.constraintlayout.widget.ConstraintLayout>

        </LinearLayout>

        <LinearLayout
            android:id="@+id/ishaaCard"
            android:layout_width="match_parent"
            android:layout_height="55dp"
            android:layout_marginStart="1dp"
            android:layout_marginEnd="1dp"
            app:cardBackgroundColor="?attr/backgroundColor"
            app:layout_constraintTop_toBottomOf="@+id/maghribCard"
            app:cardCornerRadius="0dp"
            app:strokeWidth="0dp"
            tools:layout_editor_absoluteX="8dp">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <LinearLayout
                    android:id="@+id/isha_left_container"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="10dp"
                    android:orientation="horizontal"
                    android:gravity="center_vertical"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent">

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:background="@drawable/symbol_ishaa"/>

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="12dp"
                        android:orientation="vertical"
                        android:gravity="start">

                        <TextView
                            android:id="@+id/icha_label_text_view"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/ICHA"
                            android:textAppearance="@style/prayer_name"
                            android:textSize="15sp"
                            android:textStyle="bold" />

                        <TextView
                            android:id="@+id/icha_timing_text_view"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="2dp"
                            android:text="6:54 PM"
                            android:textSize="12sp"
                            android:textAppearance="@style/prayer_name" />
                    </LinearLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="2dp"
                    android:orientation="horizontal"
                    android:gravity="center"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="parent">

                    <com.google.android.material.button.MaterialButton
                        android:id="@+id/isha_track_button"
                        android:layout_width="wrap_content"
                        android:layout_height="32dp"
                        android:layout_marginEnd="8dp"
                        android:text="Track"
                        android:textSize="11sp"
                        android:textColor="@android:color/white"
                        android:paddingStart="12dp"
                        android:paddingEnd="12dp"
                        android:minWidth="0dp"
                        android:insetTop="0dp"
                        android:insetBottom="0dp"
                        android:visibility="visible"
                        app:cornerRadius="16dp"
                        app:backgroundTint="@color/salah_track_button"
                        style="@style/Widget.MaterialComponents.Button" />

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/icha_call_constraint_layout"
                        android:layout_width="38dp"
                        android:layout_height="38dp"
                        android:layout_marginStart="4dp">

                        <ImageView
                            android:id="@+id/icha_call_image_view"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@drawable/symbol_notification"
                            android:layout_gravity="center"
                            android:contentDescription="@string/title_adhan"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent"/>
                    </androidx.constraintlayout.widget.ConstraintLayout>
                </LinearLayout>

            </androidx.constraintlayout.widget.ConstraintLayout>

        </LinearLayout>

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/method_row_constraint_layout"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintTop_toBottomOf="@+id/praters365_timings_box"
        android:layout_marginTop="10dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHeight_percent="0.10"
        app:layout_constraintStart_toStartOf="parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/method_layout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:gravity="center"
            android:orientation="horizontal"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.0">

            <ImageView
                android:layout_width="15dp"
                android:layout_height="15dp"
                android:layout_marginEnd="4dp"
                android:layout_marginRight="4dp"
                android:contentDescription="@string/your_location"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/calculation_method_text_view"
                app:layout_constraintTop_toTopOf="parent"
                app:srcCompat="@drawable/ic_information_24dp" />

            <TextView
                android:id="@+id/calculation_method_text_view"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="32dp"
                android:layout_marginLeft="32dp"
                android:ellipsize="end"
                android:fontFamily="sans-serif-condensed"
                android:gravity="start|top"
                android:maxLines="1"
                android:scrollHorizontally="true"
                android:textColor="?attr/textColorSecondary"
                android:textSize="@dimen/text_middle"
                android:textStyle="normal"
                app:autoSizeTextType="uniform"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.0"
                tools:text="L\'Union des Organisations Islamiques de France (UOIF)" />
        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.constraintlayout.widget.ConstraintLayout>


    <androidx.constraintlayout.widget.ConstraintLayout
        android:visibility="gone"
        android:id="@+id/first_timing_constraint_layout"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:padding="5dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/second_timing_constraint_layout"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintWidth_percent="0.333">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/first_timing_title_constraint_layout"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            app:layout_constraintBottom_toTopOf="@+id/first_timing_clock_constraint_layout"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHeight_percent="0.15"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <TextView
                android:id="@+id/fajr_label_text_vieww"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                tools:text="A'Sobh"
                android:fontFamily="sans-serif-condensed"
                android:gravity="center"
                android:maxLines="1"
                android:textColor="?attr/textColorPrimary"
                android:textSize="@dimen/text_middle"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />
        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/first_timing_clock_constraint_layout"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            app:layout_constraintBottom_toTopOf="@+id/first_timing_text_constraint_layout"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHeight_percent="0.65"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/first_timing_title_constraint_layout">


        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/first_timing_text_constraint_layout"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            app:layout_constraintBottom_toBottomOf="parent"
