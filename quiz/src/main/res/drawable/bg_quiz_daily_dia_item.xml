<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_selected="true">
        <shape>
            <corners android:radius="@dimen/dp_6"/>
            <gradient android:startColor="#75B66B" android:endColor="#75B66B" android:angle="135"/>
            <stroke android:color="#1AFFF3F3" android:width="@dimen/dp_2"/>
        </shape>
    </item>
    <item android:state_enabled="false">
        <shape>
            <corners android:radius="@dimen/dp_6"/>
            <solid android:color="#477A3F"/>
            <stroke android:width="@dimen/dp_2" android:color="#477A3F"/>
        </shape>
    </item>
    <item>
        <shape>
            <gradient android:startColor="#75B66B" android:endColor="#75B66B" android:angle="90"/>
            <corners android:radius="@dimen/dp_6"/>
            <stroke android:color="#1AFFFFFF" android:width="@dimen/dp_2"/>
        </shape>
    </item>
</selector>